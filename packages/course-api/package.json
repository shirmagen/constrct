		"copyfiles": "^2.1.1",
		"nodemon": "^1.19.3"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"dotenv-extended": "^2.4.0",
		"email-address": "^1.2.2",
